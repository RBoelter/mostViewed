msgid "admin.scheduledTask.mostViewed"
msgstr "mostViewed"

msgid "plugins.generic.most.viewed.title"
msgstr "Most Viewed Articles"

msgid "plugins.generic.most.viewed.desc"
msgstr "Show the most-viewed and downloaded articles on the journal's homepage."

msgid "plugins.generic.most.viewed.headline"
msgstr "Most Read Articles"

msgid "plugins.generic.most.viewed.head"
msgstr "Section Heading"

msgid "plugins.generic.most.viewed.head.desc"
msgstr "The heading that should be displayed above the most read articles section. Leave this field blank to display \"Most Read Articles\"."

msgid "plugins.generic.most.viewed.days"
msgstr "Number of Days"

msgid "plugins.generic.most.viewed.days.desc"
msgstr "Includes views and downloads from this number of days. For example, enter 30 to get the most read articles over the last 30 days."

msgid "plugins.generic.most.viewed.amount"
msgstr "Number of Articles"

msgid "plugins.generic.most.viewed.amount.desc"
msgstr "How many articles should be displayed."

msgid "plugins.generic.most.viewed.years"
msgstr "Exclude Old Articles"

msgid "plugins.generic.most.viewed.years.desc"
msgstr "Exclude articles published more than this number of years ago. For example, enter 2 if you only want to include articles published in the last two years. Leave this blank to include all published articles."

msgid "plugins.generic.most.viewed.position"
msgstr "Display Location"

msgid "plugins.generic.most.viewed.position.desc"
msgstr "If you enable this setting, the plugin will attempt to display the most read articles at the bottom of your journal homepage. This may not work on all themes."

msgid "plugins.generic.most.viewed.position.check"
msgstr "Try to move the most read articles to the bottom of the journal homepage"
